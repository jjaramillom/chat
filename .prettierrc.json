{
	"plugins": [
		"prettier-plugin-tailwindcss",
		"@ianvs/prettier-plugin-sort-imports"
	],
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": false,
	"jsxSingleQuote": true,
	"importOrder": [
		"^(react/(.*)$)|^(react$)",
		"<THIRD_PARTY_MODULES>",
		"",
		"^[~/].*",
		"^[./].*"
	]
}
